# AIA Cognitive-Neural Development Configuration
# =============================================

# Environment
ENVIRONMENT=development
DEBUG=true
NODE_ENV=development
PYTHON_ENV=development
LOG_LEVEL=debug

# Database Configuration (Cognitive-Enhanced)
DATABASE_URL=postgresql://aia_user:aia_pass@aia-postgres-cognitive:5432/aia_cognitive_dev
REDIS_URL=redis://aia-redis-cognitive:6379/0
NEO4J_URI=bolt://aia-neo4j-cognitive:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=cognitive_password

# Security (Development - Enhanced for Cognitive Features)
JWT_SECRET_KEY=cognitive_neural_dev_jwt_secret_key_not_secure_for_production
API_KEY_SECRET=cognitive_neural_dev_api_secret_key_not_secure_for_production
ENCRYPTION_KEY=cognitive_neural_32_chars_min_key
SESSION_SECRET=cognitive_neural_session_secret_key_not_secure_for_production

# Cognitive-Neural Feature Flags
COGNITIVE_NEURAL_ENABLED=true
ML_PERSONALIZATION_ENABLED=true
BIOMETRIC_PROCESSING_ENABLED=true
REAL_TIME_ADAPTATION_ENABLED=true
PRIVACY_PRESERVING_ML=true
FEDERATED_LEARNING_ENABLED=true
A_B_TESTING_ENABLED=true
ADVANCED_ANALYTICS_ENABLED=true
NEURO_COGNITIVE_3D_ENABLED=true
EMOTIONAL_INTELLIGENCE_ENABLED=true

# ML/AI Configuration
TENSORFLOW_ENABLE_XLA=1
TF_ENABLE_ONEDNN_OPTS=1
PYTORCH_CUDA_ALLOC_CONF=max_split_size_mb:128
ML_MODEL_PATH=/app/models
HUGGINGFACE_CACHE_DIR=/app/models/huggingface

# Privacy Settings (Development)
DIFFERENTIAL_PRIVACY_EPSILON=1.0
PRIVACY_BUDGET_DAILY=10.0
DATA_RETENTION_DAYS=30
ANONYMIZATION_ENABLED=true
CONSENT_REQUIRED=true

# Performance Configuration (Enhanced for ML)
MAX_WORKERS=4
WORKER_TIMEOUT=120
MAX_CONNECTIONS=200
POOL_SIZE=20
ML_BATCH_SIZE=32
INFERENCE_TIMEOUT=5000
REAL_TIME_PROCESSING_TIMEOUT=100

# Cognitive Analytics
COGNITIVE_SAMPLING_RATE=10.0
BIOMETRIC_SAMPLING_RATE=5.0
ADAPTATION_THRESHOLD=0.7
LEARNING_RATE_ADJUSTMENT=0.01
CONFIDENCE_THRESHOLD=0.8

# Feature Flags for Local Development
MOCK_BIOMETRIC_DATA=true
MOCK_EYE_TRACKING=true
MOCK_VOICE_ANALYSIS=true
SIMULATE_COGNITIVE_LOAD=true
ENABLE_DEBUG_VISUALIZATIONS=true

# API Keys (Add your own for enhanced functionality)
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here
# GOOGLE_API_KEY=your_google_key_here
# HUGGINGFACE_API_TOKEN=your_hf_token_here

# Model Serving
TENSORFLOW_SERVING_URL=http://aia-ml-serving:8501
MODEL_SERVING_TIMEOUT=5000
MODEL_CACHE_TTL=3600

# Monitoring & Observability
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true
METRICS_EXPORT_INTERVAL=30
HEALTH_CHECK_INTERVAL=60

# A/B Testing
AB_TESTING_CONFIDENCE_LEVEL=0.95
AB_TESTING_STATISTICAL_POWER=0.8
AB_TESTING_MIN_SAMPLE_SIZE=100

# Real-time Features
WEBSOCKET_ENABLED=true
REAL_TIME_UPDATES=true
STREAMING_ANALYTICS=true
LIVE_ADAPTATION=true

# Storage
MINIO_ACCESS_KEY=cognitive_minio
MINIO_SECRET_KEY=cognitive_minio_secret
MINIO_ENDPOINT=aia-minio:9000
MODEL_STORAGE_BUCKET=cognitive-models
DATA_STORAGE_BUCKET=cognitive-data

# CORS for local development (Enhanced)
CORS_ORIGINS=http://localhost:3000,http://localhost:8000,http://localhost:3001,http://localhost:9090,http://localhost:8888
ALLOWED_HOSTS=localhost,127.0.0.1,aia-frontend-cognitive,aia-api-cognitive

# WebRTC for real-time communication
WEBRTC_ENABLED=true
WEBRTC_ICE_SERVERS='[{"urls": "stun:stun.l.google.com:19302"}]'

# Jupyter Lab Integration
JUPYTER_ENABLED=true
JUPYTER_BASE_URL=http://aia-jupyter-lab:8888

# Development Tools
ENABLE_SQL_LOGGING=true
ENABLE_ML_DEBUGGING=true
ENABLE_PERFORMANCE_PROFILING=true
ENABLE_MEMORY_TRACKING=true

# Cognitive Processing Thresholds
HIGH_COGNITIVE_LOAD_THRESHOLD=0.8
LOW_FOCUS_THRESHOLD=0.3
HIGH_STRESS_THRESHOLD=0.7
FATIGUE_THRESHOLD=0.6
ENGAGEMENT_MIN_THRESHOLD=0.4

# Adaptation Parameters
UI_COMPLEXITY_REDUCTION_FACTOR=0.3
ANIMATION_SPEED_REDUCTION=0.5
BREAK_SUGGESTION_INTERVAL=1800  # 30 minutes
HELP_PROACTIVITY_THRESHOLD=0.5

# Email Configuration (Mock for development)
EMAIL_FROM_ADDRESS=cognitive@localhost
EMAIL_FROM_NAME=AIA Cognitive Development
SENDGRID_MOCK=true